{
  "name": "hedgenight",
  "version": "1.0.0",
  "description": "Hedgenight timer app",
  "main": "server.js",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"npm:server\" \"npm:client\"",
    "server": "nodemon server.js --ignore client 'client/*'",
    "seed": "node scripts/seedDB.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "postinstall": "cd client && npm i"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "api": "^2.4.4",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-react-app": "^3.1.2",
    "bcrypt": "^5.0.0",
    "bootstrap": "^4.5.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "hh-mm-ss": "^1.2.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.13",
    "pretty-ms": "^7.0.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hunterwk/hedgenight.git"
  },
  "bugs": {
    "url": "https://github.com/hunterwk/hedgenight/issues"
  },
  "homepage": "https://github.com/hunterwk/hedgenight#readme"
}
